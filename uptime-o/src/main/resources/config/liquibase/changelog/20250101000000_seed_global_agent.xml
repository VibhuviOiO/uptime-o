<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="20250101000000-1" author="system">
        <loadData
            file="config/liquibase/data/region.csv"
            separator=";"
            tableName="regions"
            usePreparedStatements="true">
            <column name="id" type="numeric"/>
            <column name="name" type="string"/>
            <column name="region_code" type="string"/>
            <column name="group_name" type="string"/>
        </loadData>
    </changeSet>

    <changeSet id="20250101000000-2" author="system">
        <loadData
            file="config/liquibase/data/agent.csv"
            separator=";"
            tableName="agents"
            usePreparedStatements="true">
            <column name="id" type="numeric"/>
            <column name="name" type="string"/>
            <column name="region_id" type="numeric"/>
        </loadData>
    </changeSet>

</databaseChangeLog>
