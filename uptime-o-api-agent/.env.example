# API Configuration (Required)
API_BASE_URL=http://localhost:8080
API_KEY=uptimeo_YOUR_API_KEY_HERE
AGENT_ID=2
DATACENTER_ID=1

# Health Server Port (Optional - defaults to 9090)
HEALTH_PORT=9090

# Config Reload Interval (Optional - defaults to 24h)
# Examples: 5m, 1h, 24h
CONFIG_RELOAD_INTERVAL=24h

# Database Connection (Optional - only needed for distributed locking)
# If not set, agent will run without distributed lock
DB_CONN_STRING=postgres://uptimeo:uptimeo@localhost:5432/uptimeo?sslmode=disable
